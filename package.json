{
  "name": "json-ref-lite",
  "version": "1.0.62",
  "description": "Extremely light weight module to resolve jsonschema '$ref' references (many variants: remote, local, jsonpointer, circular/graph structures, function evaluation)",
  "main": "index.js",
  "dependencies": {
    "property-expr": "^1.3.1"
  },
  "devDependencies": {},
  "scripts": {
    "prepublish": "coffee -c index.coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://coderofsalvation@github.com/coderofsalvation/json-ref-lite.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coderofsalvation/json-ref-lite/issues"
  },
  "scripts":{
    "browser": "/bin/bash -c 'coffee -c *.coffee test/*.coffee && browserify -r .:json-ref-lite > json-ref-lite.min.js'",
    "browser.min": "/bin/bash -c 'coffee -c *.coffee test/*.coffee && browserify -r .:json-ref-lite | uglifyjs -o json-ref-lite.min.js'"
  },
  "browser":{
    "sync-request":false,
    "fs":false
  },
  "keywords": [
    "json",
    "jsonschema",
    "ref",
    "json pointers",
    "jsonpointer",
    "jsonref",
    "$ref",
    "resolve",
    "remote reference",
    "json jsonschema ref json pointers jsonref remote reference lightweight",
    "circular",
    "graph"
  ],
  "homepage": "https://github.com/coderofsalvation/json-ref-lite"
}
